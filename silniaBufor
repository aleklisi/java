import java.math.*;
import java.util.*;

class WynikiSilni {
	int [] argument;
	BigInteger [] wynik;
	int najstarszyElement;
	int rozmiarBufora = 10;
	WynikiSilni(){
			int [] argument = new int[rozmiarBufora];
			BigInteger [] wynik = new BigInteger[rozmiarBufora];
		}
		void dodajWynikSilni(int arg, BigInteger wyn){
			argument[najstarszyElement] = arg;
			wynik[najstarszyElement] = wyn;
			najstarszyElement = ((najstarszyElement + 1) % rozmiarBufora);
		}
}


class Funkcyje{
	WynikiSilni bufor;
	Funkcyje(){
		WynikiSilni bufor = new WynikiSilni();
	}
	 BigInteger liczSilnie (int arg){
		if (arg < 0){
			throw new RuntimeException(" nie mize");
		}
		BigInteger wynik = new BigInteger("1");
		for (int i = 2; i <= arg; i++){
			
			wynik = wynik.multiply(new BigInteger("" + i));
		}
		bufor.dodajWynikSilni(arg,wynik);
	return wynik;
	}
}

public class SilniaAlek {
	public static void main(String []args){
		Funkcyje f = new Funkcyje();
		/*if (args.length <= 0){
			System.out.println("wywolaj program z wlasciwa liczba argumentow");
		}
		else{*/
		String pierwszyArg = "7";
			try{
			Integer atgumentSilni = new Integer(pierwszyArg);
			System.out.println(f.liczSilnie(atgumentSilni));
			}
			catch(NumberFormatException e){
				System.out.println("nie podales liczby");
			}
		//}
		
	}
}

